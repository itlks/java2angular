    </component>
  
	<component>
	    <component-type>com.santander.component.datatreeview.UIDataTreeView</component-type>
	    <component-class>com.santander.component.datatreeview.DataTreeView</component-class>
    </component>
    
    <component>
	    <component-type>com.santander.component.treetableview.UITreeTableView</component-type>
	    <component-class>com.santander.component.treetableview.TreeTableView</component-class>
    </component>
    
